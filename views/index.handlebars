<h1>Eat-Da-Burger!</h1>
<ul>
  
  {{#each burgers}}
     {{#unless devoured}}
      {{> burgers/burger-block devoured=false}}
    {{/unless}}
  {{/each}}

</ul>

<ul>  
  {{#each burgers}}
    {{#if devoured}}
      {{> burgers/burger-block devoured=true}}
    {{/if}}
  {{/each}}
</ul>



